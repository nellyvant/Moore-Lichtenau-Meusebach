}
ggplot(df_sub, aes(Datum, Wassertiefe, color = factor(Messpunkt))) +
geom_line(linewidth = 1) +
geom_point(size = 2) +
labs(title = titel,
x = "Datum", y = "Wassertiefe (cm)", color = "Messstelle") +
theme_minimal() +
