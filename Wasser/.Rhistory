if (is.null(x_name)) {
x_name <- "Messpunkt"  # Standard
}
ggplot(df_sub, aes(x = factor(Messpunkt), y = Wassertiefe, fill = factor(Messpunkt))) +
geom_boxplot() +
labs(
title = titel,
x = x_name,           # hier wird der flexible Name verwendet
y = "Wassertiefe (cm)",
fill = "Messstelle"
) +
theme_minimal()
}
