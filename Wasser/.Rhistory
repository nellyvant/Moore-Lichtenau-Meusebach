summarise(
sd_WT = sd(Wassertiefe, na.rm = TRUE),
range_WT = max(Wassertiefe, na.rm = TRUE) - min(Wassertiefe, na.rm = TRUE)
) %>%
arrange(desc(sd_WT))
WT_var_mp
